{
  "name": "nestjs-codebase-structure",
  "private": true,
  "version": "1.0.0",
  "description": "NestJS Starter REST API Repository",
  "license": "MIT",
  "contributors": [],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 nest start --watch --preserveWatchOutput",
    "dev:production": "NODE_ENV=production NODE_OPTIONS=--max-old-space-size=4096 nest start --watch --preserveWatchOutput",
    "start:stagging": "NODE_ENV=stagging nest start --watch --preserveWatchOutput",
    "start:production": "NODE_ENV=production nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register",
    "cli:prod": "node dist/src/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -d ormconfig.ts migrations/$npm_config_name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@azure/msal-node": "^2.13.0",
    "@hapi/joi": "17.1.1",
    "@liaoliaots/nestjs-redis": "^8.2.2",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "2.0.0",
    "@nestjs/cache-manager": "1.0.0",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "2.3.4",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "2.0.2",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "2.2.3",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/passport": "1.0.12",
    "@types/uuid": "8.3.4",
    "@vendia/serverless-express": "4.10.4",
    "aws-sdk": "^2.1585.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "5.2.3",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cross-fetch": "^4.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "16.3.1",
    "formidable": "^3.5.1",
    "generate-password": "^1.7.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.3.2",
    "kafkajs": "2.2.4",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "numjs": "^0.16.1",
    "openai": "^4.6.0",
    "passport": "^0.6.0",
    "passport-google-oauth2": "0.2.0",
    "passport-jwt": "4.0.1",
    "pdfmake": "^0.2.7",
    "pg": "^8.8.0",
    "querystring": "0.2.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "swagger-ui-express": "4.5.0",
    "twilio": "^4.23.0",
    "typeorm": "0.3.17",
    "uuid": "9.0.0",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "^10.4.1",
    "@types/aws-lambda": "8.10.119",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "18.17.5",
    "@types/passport-jwt": "3.0.9",
    "@types/pg": "8.10.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "googleapis": "^128.0.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8",
    "serverless-offline": "12.0.4",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
